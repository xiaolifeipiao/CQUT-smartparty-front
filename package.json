{
  "name": "smartparty-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:prod": "cross-env NODE_ENV=production dotenv -e .env.prod vue-cli-service build",
    "build:dev": "cross-env NODE_ENV=development dotenv -e .env.prod vue-cli-service build",
    "json_server": "json-server src/mock/db.js --watch",
    "mock": "cd src/mock && ts-node-dev index.ts",
    "serve:dev": "cross-env NODE_ENV=development dotenv -e .env.dev vue-cli-service serve",
    "serve:prod": "cross-env NODE_ENV=production dotenv -e .env.prod vue-cli-service serve"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-plus": "^1.0.2-beta.48",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "yue.css": "^0.4.0"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "mockjs": "^1.1.0",
    "node-sass": "^4.12.0",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "typescript": "~4.1.5",
    "vue-cli-plugin-element-plus": "0.0.13",
    "vue-loader-v16": "^16.0.0-beta.5.4",
    "webpack": "^4.44.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
